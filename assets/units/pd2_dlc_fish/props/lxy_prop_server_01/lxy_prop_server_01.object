<dynamic_object>
	<diesel materials="units/pd2_dlc_fish/props/lxy_prop_server_01/lxy_prop_server_01" orientation_object="rp_lxy_prop_server_01" />
	<sequence_manager file="units/pd2_dlc_fish/props/lxy_prop_server_01/lxy_prop_server_01" />
	<animations>
