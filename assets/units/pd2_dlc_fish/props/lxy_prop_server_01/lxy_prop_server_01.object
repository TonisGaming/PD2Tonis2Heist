	</effects>
	
	<graphics>	    
		<object name="g_door_01" enabled="true" />
		<object name="g_door_02" enabled="true" />
		<object name="g_beam_01" enabled="true" />
		<object name="g_beam_02" enabled="true" />
		<object name="e_e" />
