			<object name="g_lamp_blue_01" enabled="true" />
			<object name="g_lamp_blue_02" enabled="true" />	
            <object name="g_blue_glow" enabled="true" />			
		</graphic_group>
		<graphic_group name="over_warmed" enabled="false" >
			<object name="g_lamp_red_01" enabled="true" />
			<object name="g_lamp_red_02" enabled="true" />	
            <object name="g_red_glow" enabled="true" />			
		</graphic_group>		
	</graphics>
