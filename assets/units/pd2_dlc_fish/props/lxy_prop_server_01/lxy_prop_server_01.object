	<bodies>
		<body name="body_static_01" enabled="true" template="animated">
			<object name="rp_lxy_prop_server_01" />
			<object name="c_box_01" collision_type="box" padding="-2.5" />
		</body>
		<body name="body_static_02" enabled="true" template="animated">
			<object name="rp_lxy_prop_server_01" />
			<object name="c_box_02" collision_type="box" padding="-2.5" />
