<?xml version="1.0"?>
<dynamic_object>
	<diesel materials="units/payday2/props/bnk_prop_security_moneycounter/bnk_prop_security_moneycounter" orientation_object="rp_bnk_prop_security_moneycounter" />
	<sequence_manager file="units/payday2/props/bnk_prop_security_moneycounter/bnk_prop_security_moneycounter" />
	<animations>
		<animation_group name="anim">
			<object name="align_front_door" />
		</animation_group>
	</animations>
	<bodies>
		<body name="body_base" enabled="true" template="static">
			<object name="rp_bnk_prop_security_moneycounter" />
			<object name="c_c" collision_type="mesh_mopp" />
		</body>
		<body name="body_box" enabled="false" template="static">
			<object name="rp_bnk_prop_security_moneycounter" />
			<object name="c_box" collision_type="box" />
		</body>
		<body name="body_front_door" enabled="true" template="animated">
			<object name="align_front_door" />
			<object name="c_front_door" collision_type="box" />
		</body>
	</bodies>
	<decal_surfaces default_material="metal">
		<decal_mesh name="g_front_door" 	enabled="true" material="metal"/>
		<decal_mesh name="g_box" 			enabled="true" material="plastic"/>
		<decal_mesh name="g_g" 				enabled="true" material="metal"/>
	</decal_surfaces>
	<graphics>
		<graphic_group name="ghosticongroup" 	enabled="true" culling_object="g_lockpick_ghost">
			<lod_object name="ghost_icon">
					<object name="g_lockpick_ghost" 		enabled="true"  max_distance="450"  max_draw_lod="0" />
					<object name="g_lockpick_ghost_lod1" 	enabled="true"  lod="1" />
			</lod_object>
		</graphic_group>
		<object name="g_front_door" 	enabled="true"  />
		<object name="g_box" 			enabled="false"  />
		<object name="g_g" 				enabled="true"  />
	</graphics>
</dynamic_object>