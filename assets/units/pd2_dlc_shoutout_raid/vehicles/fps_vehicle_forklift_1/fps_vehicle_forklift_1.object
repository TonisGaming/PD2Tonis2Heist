<dynamic_object>
	<diesel materials="units/pd2_dlc_shoutout_raid/vehicles/fps_vehicle_forklift_1/fps_vehicle_forklift_1" orientation_object="rp_fps_vehicle_forklift_1" />
	<sequence_manager file="units/pd2_dlc_shoutout_raid/vehicles/fps_vehicle_forklift_1/fps_vehicle_forklift_1" />
	<animations>
		<animation_group name="ag_speedometer" enabled="false">
				<object name="a_speedometer" />
		</animation_group>
		<animation_group name="ag_rpm_meter" enabled="false">
				<object name="a_rpm_meter" />
		</animation_group>
		<animation_group name="anim_steering_wheel_left" enabled="false">
				<object name="anim_steering_wheel_left" />
		</animation_group>
		<animation_group name="anim_steering_wheel_right" enabled="false">
				<object name="anim_steering_wheel_right" />
		</animation_group>
	</animations>
	<bodies>
		<body name="body_chassis" enabled="false" template="vehicle" collision_script_filter="1" collision_script_quiet_time="0.5" collision_script_tag="vehicle_collision" >
			<object name="anim_body" />
			<object name="c_convex_01"	collision_type="convex" />
			<object name="c_convex_02"	collision_type="convex" />
			<object name="c_box_01"		collision_type="box"  	padding="-2.5" />
			<object name="c_box_02"		collision_type="box"  	padding="-2.5" />
			<object name="c_box_03"		collision_type="box"  	padding="-2.5" />
			<object name="c_box_04"		collision_type="box"  	padding="-2.5" />
			<object name="c_box_05"		collision_type="box"  	padding="-2.5" />
			<object name="c_box_06"		collision_type="box"  	padding="-2.5" />
			<object name="c_box_07"		collision_type="box"  	padding="-2.5" />
			<object name="c_box_08"		collision_type="box"  	padding="-2.5" />
			<object name="c_box_09"		collision_type="box"  	padding="-2.5" />
			<object name="c_box_10"		collision_type="box"  	padding="-2.5" />
			<object name="c_box_11"		collision_type="box"  	padding="-2.5" />
			<object name="c_box_12"		collision_type="box"  	padding="-2.5" />
			<object name="c_box_13" 	collision_type="box" 	padding="-2.5"/>
			<object name="c_box_14" 	collision_type="box" 	padding="-2.5"/>
		</body>
<!-- /// Static //////////////////////////////////////////////////////////// -->
		<body name="body_static_vehicle" enabled="true" template="animated" >
			<object name="c_c" collision_type="mesh_mopp" />
		</body>
	</bodies>
	<effects>
		<effect_spawner
			name="es_smoke_med"
			enabled="false"
			object="v_repair_engine"
			effect="effects/payday2/particles/vehicle/vehicle_damage_small_smoke"
		/>
		<effect_spawner
			name="es_smoke_full"
			enabled="false"
			object="v_repair_engine"
			effect="effects/payday2/particles/vehicle/vehicle_damage_big_smoke"
		/>
	</effects>
	<decal_surfaces default_material="metal_chassis">
		<decal_mesh name="dm_cloth"		enabled="true" material="cloth"/>
		<decal_mesh name="dm_fl_rubber"	enabled="true" material="rubber"/>
		<decal_mesh name="dm_fr_rubber"	enabled="true" material="rubber"/>
		<decal_mesh name="dm_rl_rubber"	enabled="true" material="rubber"/>
		<decal_mesh name="dm_rr_rubber"	enabled="true" material="rubber"/>
		<decal_mesh name="dm_steel"		enabled="true" material="steel"/>
	</decal_surfaces>
	<graphics>
		<graphic_group name="grp_vehicle" enabled="true">
			<object name="g_decals"				enabled="true"/>
			<object name="g_forklift"			enabled="true"/>
			<object name="g_interior"			enabled="true"/>
			<object name="g_tire_front_left"	enabled="true"/>
			<object name="g_tire_front_right"	enabled="true"/>
			<object name="g_tire_rear_left"		enabled="true"/>
			<object name="g_tire_rear_right"	enabled="true"/>

			<object name="g_fps_steering_wheel"	enabled="false"/>
			<object name="g_steering_wheel"		enabled="true"/>

			<object name="s_s"					enabled="true" shadow_caster="true"/>
			<object name="s_tire_front_left"	enabled="true" shadow_caster="true"/>
			<object name="s_tire_front_right"	enabled="true" shadow_caster="true"/>
			<object name="s_tire_rear_left"		enabled="true" shadow_caster="true"/>
			<object name="s_tire_rear_right"	enabled="true" shadow_caster="true"/>

			<object name="g_lootbag1"			enabled="false" shadow_caster="true"/>
			<object name="g_explosivebag1"		enabled="false" shadow_caster="true"/>
			<object name="g_lootbag2"			enabled="false" shadow_caster="true"/>
			<object name="g_explosivebag2"		enabled="false" shadow_caster="true"/>
			<object name="g_lootbag3"			enabled="false" shadow_caster="true"/>
			<object name="g_explosivebag3"		enabled="false" shadow_caster="true"/>
		</graphic_group>

		<graphic_group name="grp_icon_entry" enabled="true">
			<lod_object name="lod_icon_drive">
				<object name="g_icon_door_drive_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_door_drive_lod1" enabled="true" />
			</lod_object>
			<lod_object name="lod_icon_front_r_pass">
				<object name="g_icon_door_front_r_pass_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_door_front_r_pass_lod1" enabled="true" />
			</lod_object>
		</graphic_group>

		<graphic_group name="grp_icon_loot" enabled="true">
			<lod_object name="lod_icon_loot">
				<object name="g_icon_loot_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_loot_lod1" enabled="true" />
			</lod_object>
		</graphic_group>

		<graphic_group name="grp_icon_repair" enabled="true">
			<lod_object name="lod_icon_repair">
				<object name="g_icon_repair_lod0" enabled="true"  max_distance="450" />
				<object name="g_icon_repair_lod1" enabled="true" />
			</lod_object>
		</graphic_group>
	</graphics>

	<lights>
		<light name="light_back"
			enabled="false"
			editable="false"
			type="omni|specular"
			multiplier="searchlight"
			far_range="50"
			falloff="1.0"
		/>
	</lights>

	<vehicle name="vhc_vehicle">
		<!-- IMPORTANT: suspension supported_mass must be 1/4 of chassis mass, if not vehicle behavior is undefined -->
		<!-- IMPORTANT: downforce should not go above 20, or it will compress the suspension and make the car unable to handle bumps -->
		<!-- IMPORTANT: use inertia multipliers with care -->
		<chassis body="body_chassis" mass="2000" center_of_mass_offset="0.0 0.0 0.5" inertia_multiplier="1.0 1.1 1.0" downforce_strength="1.0" drag_strength="0.0"/>
		<wheels>
			<wheel spin_object="anim_tire_front_left"  turn_object=""	radius="0.325" width="0.15" mass="30" transmission_ratio="1.1" is_steering="false" max_steering="0.1" braking_torque="3000" handbrake_torque="0" />
			<wheel spin_object="anim_tire_front_right" turn_object=""	radius="0.325" width="0.15" mass="30" transmission_ratio="1.1" is_steering="false" max_steering="0.1" braking_torque="3000" handbrake_torque="0" />
			<wheel spin_object="anim_tire_rear_left"   turn_object="" 	radius="0.253" width="0.15" mass="30" transmission_ratio="1.1" is_steering="true" max_steering="45" braking_torque="3000" handbrake_torque="4000" />
			<wheel spin_object="anim_tire_rear_right"  turn_object="" 	radius="0.253" width="0.15" mass="30" transmission_ratio="1.1" is_steering="true" max_steering="45" braking_torque="3000" handbrake_torque="4000" />
		</wheels>
		<suspension max_compression="0.1" max_extension="0.1" spring_strength="55000" damper_rate="5000" supported_mass="500" />
		<gearbox switch_time="0.1" ratio_multiplier="4.0">
			<ratios>
				<ratio value="-4.0" />
				<ratio value="0.0" />
				<ratio value="3.75" />
			</ratios>
		</gearbox>
		<engine max_torque="300" max_rpm="1800"/>
	</vehicle>

</dynamic_object>
